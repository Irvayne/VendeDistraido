<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.ufpi.easii.es.vendedistraido.view.cliente.MapaClienteActivity">
    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="br.ufpi.easii.es.vendedistraido.view.cliente.MapaClienteActivity" />
    <LinearLayout
        android:layout_margin="@dimen/activity_vertical_margin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            style="@style/NoSelectTab"
            android:layout_height="45dp"
            android:id="@+id/cliente_mapa_btn_lista"
            android:text="@string/lista"/>
        <Button
            style="@style/SelectTab"
            android:id="@+id/cliente_mapa_btn_mapa"
            android:text="@string/mapa"/>
    </LinearLayout>
</FrameLayout>